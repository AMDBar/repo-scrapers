				<expression>&lt;div class="imagelocandina" style="background-image: url\(([^_]+)[^\)]+\);</expression>
			</RegExp>
			<!--Fanart Grabber1-->
			<RegExp conditional="fan" input="$$1" output="&lt;url cache=&quot;\2-wall.html&quot; function=&quot;FanartGrabber&quot; &gt;http://www.movieplayer.it/serietv/\1/\2/gallery-e-trailer/wallpaper/1/&lt;/url&gt;" dest="5+">
