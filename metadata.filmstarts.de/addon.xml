<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="metadata.filmstarts.de"
       name="FilmStarts"
       version="1.0.9"
       provider-name="Team XBMC">
  <requires>
    <import addon="xbmc.metadata" version="1.0"/>
    <import addon="metadata.common.themoviedb.org" version="1.0.7"/>
  </requires>
  <extension point="xbmc.metadata.scraper.movies"
             language="de"
             library="filmstarts.xml"/>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Filmstarts.de Movie Scraper</summary>
    <summary lang="hu">Filmstarts.de filmadat leolvasó</summary>
    <summary lang="kr">Filmstarts.de 영화 스크래퍼</summary>
    <summary lang="nl">Filmstarts.de Film Scraper</summary>
    <summary lang="pl">Scraper filmów Filmstarts.de</summary>
    <summary lang="pt">Scraper de filmes Filmstarts.de</summary>
    <summary lang="se">Filmstarts.de Filmskrapa</summary>
    <description lang="en">Download Movie information from www.filmstarts.de</description>
    <description lang="hu">Film információk letöltése a www.filmstarts.de webhelyről</description>
    <description lang="kr">www.filmstarts.de 에서 영화 정보 다운로드</description>
    <description lang="nl">Download film informatie van www.filmstarts.de</description>
    <description lang="pl">Pobieraj informacje o filmach z www.filmstarts.de</description>
    <description lang="pt">Descarregar informação de filmes de www.filmstarts.de</description>
    <description lang="se">Ladda ner filminformation från www.filmstarts.de</description>
    <platform>all</platform>
  </extension>
</addon>
