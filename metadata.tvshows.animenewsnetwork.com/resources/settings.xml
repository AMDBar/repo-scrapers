<?xml version="1.0" encoding="UTF-8"?>
<settings>
    <setting label="Enable All Language Casts" type="bool" id="allcasts" default="false"/>
    <setting label="Enable Unlisted Specials / 1 Episode OVA Workaround" type="bool" id="specials" default="true"/>
    <setting type="sep"/>
    <setting label="Enable TVDB Fanart" type="bool" id="fanart" default="true"/>
    <setting label="Include Alternative Titles in Fanart Search" type="bool" id="fanartalt" default="false" enable="eq(-1,true)"/>
    <setting type="sep"/>
    <setting label="Enable TVDB Banner (With ANN Thumbnail Fallback)" type="bool" id="banner" default="false" enable="!eq(1,true)"/>
    <setting label="Enable TVDB Poster (With ANN Thumbnail Fallback)" type="bool" id="poster" default="false" enable="!eq(-1,true)"/>
    <setting type="sep"/>
    <setting label="Enable TVDB Episode Details (Using Episode Title Matching)" type="bool" id="details" default="false"/>
</settings>
