<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="metadata.videobuster.de"
       name="VideoBuster"
       version="1.0.3"
       provider-name="Team XBMC">
  <requires>
    <import addon="xbmc.metadata" version="1.0"/>
    <import addon="metadata.common.themoviedb.org" version="1.0"/>
  </requires>
  <extension point="xbmc.metadata.scraper.movies"
             language="de"
             library="videobuster.xml"/>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">VideoBuster.de Movie Scraper</summary>
    <summary lang="kr">VideoBuster.de 영화 스크래퍼</summary>
    <summary lang="se">VideoBuster.de Filmskrapa</summary>
    <summary lang="nl">VideoBuster.de Film Scraper</summary>
    <description lang="en">Download Movie information from www.videobuster.de</description>
    <description lang="kr">www.videobuster.de에서 영화 정보 다운로드</description>
    <description lang="se">Ladda ner filminformation från www.videobuster.de</description>
    <description lang="nl">Download film informatie van www.videobuster.de</description>
  </extension>
</addon>
